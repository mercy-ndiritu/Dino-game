      $(document).ready(function() {
        $(".add").click(function() {
          $('<div> Upload your document here.. <div><input class="form-control" name="document[]"accept=""  type="file" ></div> <div> Enter the above document Name here..<input class="form-control" name="documentname[]" placeholder="Enter Document name e.g Id"  type="text" > </div> <div class="btn btn-danger"> <span class="rem" > <a href="javascript:void(0);" >Remove This Document </span></div>').appendTo(".contents");

        });
        $('.contents').on('click', '.rem', function() {
          $(this).parent("div").remove();
        });

      });
